%h1 Listing likes

%table#table
  %thead
    %tr
      %th Post
      %th User
      %th
      %th
      %th

  %tbody
    - @likes.each do |like|
      %tr
        %td= like.post
        %td= like.user
        %td= link_to 'Show', show_like_path(like)
        %td= link_to 'Edit', edit_like_path(like)
        %td= link_to 'Destroy', destroy_like_path(like), method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Like', new_like_path